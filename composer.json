{
    "name": "ntimyeboah/laravel-database-trigger",
    "description": "A package to add database trigger to Laravel migrations",
    "keywords": [
        "NtimYeboah",
        "trigger",
        "database",
        "laravel",
        "laravel-database-trigger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ntim Yeboah",
            "email": "ntimobedyeboah@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "NtimYeboah\\LaravelDatabaseTrigger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NtimYeboah\\LaravelDatabaseTrigger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NtimYeboah\\LaravelDatabaseTrigger\\TriggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
